{
  "compilerOptions": {
    "baseUrl": "",
    "declaration": false,
    "noImplicitAny": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es6", "dom"],
    "mapRoot": "./",
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "es5",
    "removeComments": true,
    "typeRoots": [
      "../node_modules/@types"
    ]
  },
  "files": [
    "main.ts",
    "typings.d.ts"
  ],
  "filesGlob": [
    "**/*.ts"
  ],
  "atom": {
    "rewriteTsconfig": false
  }


}
